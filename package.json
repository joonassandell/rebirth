{
  "name": "rebirth-ui",
  "version": "0.5.42",
  "author": "Joonas Ylitalo",
  "homepage": "http://joonasy.com",
  "description": "Front-end starter kit and styleguide for developing responsive, mobile first projects on the web.",
  "license": "MIT",
  "dependencies": {
    "highlight.js": "^9.12.0",
    "jquery": "^3.1.1",
    "lazysizes": "^4.0.1",
    "normalize.css": "^7.0.0"
  },
  "main": "assemblefile.js",
  "repository": "https://github.com/joonasy/rebirth.git",
  "scripts": {
    "build": "NODE_ENV=production assemble build",
    "dev": "yarn docs-dev",
    "dist": "yarn docs-dist",
    "docs-build": "NODE_ENV=production assemble docs-build",
    "docs-dev": "assemble docs-dev",
    "docs-dist": "yarn docs-build && assemble docs-server",
    "docs-deploy": "yarn docs-build && assemble docs-deploy",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build",
    "release": "npm publish --tag beta"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "assemble": "^0.24.3",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "base-watch": "^0.1.3",
    "browser-sync": "^2.23.2",
    "browserify": "^14.5.0",
    "deamdify": "^0.3.0",
    "debowerify": "~1.5.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^2.0.1",
    "gulp": "~3.9.1",
    "gulp-autoprefixer": "4.1.0",
    "gulp-changed": "^3.2.0",
    "gulp-combine-mq": "^0.4.0",
    "gulp-concat": "^2.5.2",
    "gulp-cssnano": "^2.1.0",
    "gulp-eslint": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-rev": "^8.1.1",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^3.0.0",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "~3.0.0",
    "gulp-util": "~3.0.1",
    "handlebars-helpers": "^0.10.0",
    "helper-markdown": "^1.0.0",
    "husky": "^0.14.3",
    "install": "^0.10.2",
    "js-yaml": "^3.10.0",
    "lint-staged": "^6.1.0",
    "node-fetch": "^1.7.3",
    "node-notifier": "^5.1.2",
    "prettier": "^1.15.1",
    "pretty-hrtime": "^1.0.0",
    "rimraf": "^2.5.3",
    "sass-mq": "^3.2.9",
    "through2": "^2.0.3",
    "vinyl-fs": "^3.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "env"
            ]
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      "deamdify"
    ]
  },
  "engines": {
    "node": ">=9.10.0"
  }
}
